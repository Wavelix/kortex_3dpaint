<launch>
  <!-- 设置命名空间 -->
  <arg name="robot_name" default="my_gen3_lite" />

  <!-- 直接使用 MoveIt 的官方配置（已包含模型和控制器） -->
  <include file="$(find gen3_robotiq_2f_85_move_it_config)/launch/move_group.launch" />

  
  <!-- 3. 启动你自己的节点 -->
  <!-- 启动主程序 -->
  <node pkg="kortex_3dpaint"
        name="paint_node"
        type="paint.py"
        args="__ns:=$(arg robot_name)"
        output="screen"
        launch-prefix="python3" />
</launch>
